  {
   "cell_type": "code",
   "execution_count": 56,
   "id": "f1ef416f-8bde-495e-831d-e41e3bbce4de",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Handling the mising values with the mode\n",
    "\n",
    "# Calculate the mode for SoldFlag\n",
    "sold_flag_mode = df['SoldFlag'].mode()[0]\n",
    "\n",
    "# Calculate the mode for SoldCount\n",
    "sold_count_mode = df['SoldCount'].mode()[0]\n",
    "\n",
    "# Fill missing values in SoldFlag and SoldCount with their respective modes\n",
    "df['SoldFlag'].fillna(sold_flag_mode, inplace=True)\n",
    "df['SoldCount'].fillna(sold_count_mode, inplace=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 57,
   "id": "ab6cdead-b32a-497c-8809-3116db144248",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Converting some data types as per required analysis\n",
    "# Convert SoldFlag and SoldCount to integer\n",
    "df['SoldFlag'] = df['SoldFlag'].astype(int)\n",
    "df['SoldCount'] = df['SoldCount'].astype(int)\n",
    "\n",
    "# Convert File_Type and MarketingType to category\n",
    "df['File_Type'] = df['File_Type'].astype('category')\n",
    "df['MarketingType'] = df['MarketingType'].astype('category')"
   ]
  },
